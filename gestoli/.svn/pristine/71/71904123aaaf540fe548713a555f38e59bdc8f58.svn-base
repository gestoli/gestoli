package es.caib.gestoli.logic.model;

import java.io.Serializable;
import java.util.Date;

public class QualitatNoConformitatAccio implements Serializable {

	private static final long serialVersionUID = 1L;
	private Long id;
	private Long codi;
	private String accio;
	private QualitatDescripcioPersonal responsableAccio;
	private Date dataFiPrevista;
	private Date dataTancament;
	
	private QualitatNoConformitat noConformitat;
	
	public QualitatNoConformitatAccio() {
		super();
	}
	
	public QualitatNoConformitatAccio(Long id, Long codi, String accio,
			QualitatDescripcioPersonal responsableAccio, Date dataFiPrevista, Date dataTancament, 
			QualitatNoConformitat noConformitat){
		this.id = id;
		this.codi = codi;
		this.accio = accio;
		this.responsableAccio = responsableAccio;
		this.dataFiPrevista = dataFiPrevista;
		this.dataTancament = dataTancament;
		this.noConformitat = noConformitat;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getCodi() {
		return codi;
	}

	public void setCodi(Long codi) {
		this.codi = codi;
	}

	public String getAccio() {
		return accio;
	}

	public void setAccio(String accio) {
		this.accio = accio;
	}

	public QualitatDescripcioPersonal getResponsableAccio() {
		return responsableAccio;
	}

	public void setResponsableAccio(QualitatDescripcioPersonal responsableAccio) {
		this.responsableAccio = responsableAccio;
	}

	public Date getDataFiPrevista() {
		return dataFiPrevista;
	}

	public void setDataFiPrevista(Date dataFiPrevista) {
		this.dataFiPrevista = dataFiPrevista;
	}

	public Date getDataTancament() {
		return dataTancament;
	}

	public void setDataTancament(Date dataTancament) {
		this.dataTancament = dataTancament;
	}
	
	public QualitatNoConformitat getNoConformitat() {
		return noConformitat;
	}

	public void setNoConformitat(QualitatNoConformitat noConformitat) {
		this.noConformitat = noConformitat;
	}
	
	
	
}
