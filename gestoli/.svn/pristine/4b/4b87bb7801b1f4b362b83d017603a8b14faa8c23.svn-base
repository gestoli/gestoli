/* Estilos generales */
body {
	margin: 0px;
	padding: 0px;
	text-align: center;
	font-family: Arial, Helvetica, sans-serif;
	background: #ffffff url(../img/general/fondoGeneral.jpg) top center
		repeat-x;
}

p,h1,h2,h3,h4,h5,ul,li {
	margin: 0;
	padding: 0;
}

ul {
	list-style-type: none;
}

img {
	border: none;
}

div.separadorH {
	clear: both;
	overflow: hidden;
	height: 1px;
}

div.separadorV90 {
	min-width: 90px;
	float: left;
	height: 1px;
}

div.separadorHActivo {
	clear: both;
	overflow: hidden;
	height: 15px;
}

h2.microweb {
	font-size: 28px;
	color: #DD0000;
	margin-bottom: 16px;
	position: relative;
	font-weight: normal;
}

h3.microweb {
	color: #ffffff;
	background-color: #DD0000;
	font-size: 15px;
	margin-bottom: 16px;
	padding-right: 21px;
	padding-left: 7px;
	float: left;
	position: relative;
}

strong.microweb {
	background-color: #F1CC2E;
	font-weight: normal;
}

.encuadrado fieldset {
	margin: 0;
	padding: 10px;
	border: 1px solid black;
	margin-bottom: 15px;
}

.encuadrado fieldset legend {
	color: #000000;
	font-weight: bold;
	margin: 0 0 0 5px;
	padding-left: 5px;
	padding-right: 5px;
}

form,fieldset {
	margin: 0;
	padding: 10px;
	border: none;
}

