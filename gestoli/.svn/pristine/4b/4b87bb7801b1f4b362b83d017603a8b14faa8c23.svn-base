/* campos formulario 

 <div id="nombre" class="campoForm">
    <label></label>
    <div class="bordeInput">
	 	<div id="nombreBordeFoco" class="bordeFoco">
        <input/>
		  <textarea></textarea>
		</div>
    </div>
</div>

checkbox:

<div class="etiqueta">
    <input type="checkbox" />
    <label></label>
</div> */
.campoNoRequerido,.required,.etiqueta {
	margin-bottom: 15px;
	float: left;
}

.campoNoRequerido label,.required label { /*background-color: #F9E574;*/
	padding: 2px 16px 3px 5px;
}

.campoNoRequerido .bordeInput,.required .bordeInput {
	padding: 3px;
	/*background-color: #F9E574;*/
	float: left;
}

.campoNoRequerido .bordeFoco,.campoNoRequerido .bordeFocoTextarea,.required .bordeFoco,.required .bordeFocoTextarea
	{ /*background-color: #F9E574;*/
	float: left;
}

.campoNoRequerido .bordeFoco,.required .bordeFoco {
	padding: 2px !important;
	padding: 1px 2px;
}

.campoNoRequerido .bordeFocoTextarea,.required .bordeFocoTextarea {
	padding: 1px 2px;
}

.campoNoRequerido input,.campoNoRequerido textarea,.required input,.required textarea
	{
	border: 1px solid #809DB9;
	font-size: 13px;
}

/* estructura parrafo error
<p class="error">
     <span class="capa1"></span>
     <span class="capa2"></span>
     <span class="capa3"></span>
     <span class="capa4"></span>
     <span class="capa5">
         <span class="capa6">
             Texto de error
         </span>
     </span>
 </p>
*/
p.error {
	margin-top: 5px;
	margin-bottom: 9px;
	color: #ffffff;
	font-size: 12px;
	position: relative;
	float: left;
	overflow: hidden;
}

p.error .capa1,p.error .capa2,p.error .capa3,p.error .capa4 {
	position: absolute;
	width: 7px;
	height: 7px;
	display: block;
	float: left;
}

p.error .capa1 {
	top: 0;
	left: 0;
	background: url(../img/general/microweb/error_esq_sup_izq.png) top left
		no-repeat !important;
	background-image: none;
}

p.error .capa2 {
	top: 0;
	right: 0 !important;
	right: 7px;
	background: url(../img/general/microweb/error_esq_sup_der.png) top right
		no-repeat !important;
	background-image: none;
}

p.error .capa3 {
	bottom: 0 !important;
	bottom: -9px;
	right: 0 !important;
	right: 7px;
	background: url(../img/general/microweb/error_esq_inf_der.png) bottom
		right no-repeat !important;
	background-image: none;
}

p.error .capa4 {
	bottom: 0 !important;
	bottom: -9px;
	left: 0;
	background: url(../img/general/microweb/error_esq_inf_izq.png) bottom
		left no-repeat !important;
	background-image: none;
}

p.error .capa5 {
	background-color: #DD0000;
	margin: 0px 7px 0px 7px !important;
	margin-left: 3px;
	padding: 7px 0 7px 0 !important;
	padding: 7px 0 6px 0;
	display: block;
	float: left;
}

p.error .capa6 {
	background-color: #DD0000;
	margin: 0 -7px 0 -7px;
	padding: 0 6px 0 7px;
	display: block;
	float: left;
	position: relative;
}

p.error .capaLogErr {
	background-color:#DD0000;
	display:block;
	margin:0 -7px;
	padding:0 6px 0 7px;
	position:relative;
	width:455px;
}

