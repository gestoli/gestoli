package es.caib.gestoli.front.spring;

import es.caib.gestoli.logic.model.Arxiu;
import es.caib.gestoli.logic.model.InformacioUtil;

public class InformacioUtilCommand extends InformacioUtil {
	private static final long serialVersionUID = -3454772861320876579L;
	
	private Arxiu arxiuImatgePrincipal;
	private byte[] imatgeImatgePrincipal;
	private Long imatgePrincipal;
	
	private Arxiu arxiuImatgeSecundaria;
	private byte[] imatgeImatgeSecundaria;
	private Long imatgeSecundaria;
	
	private String errorImagenes;
	
	/**
	 * Rellena los campos de este objeto con la informaci√≥n
	 * de la InformacioUtil
	 * 
	 * @param m InformacioUtil
	 */
	public void fromInformacioUtil(InformacioUtil m) {
		setId(m.getId());
		setDataAlta(m.getDataAlta());
		setActiu(m.getActiu());
		setNom(m.getNom());
		setDescripcio(m.getDescripcio());
		setNomEs(m.getNomEs());
		setDescripcioEs(m.getDescripcioEs());
		setDataInici(m.getDataInici());
		setDataFinal(m.getDataFinal());
	}		
	
	
	public Arxiu getArxiuImatgePrincipal() {
		return arxiuImatgePrincipal;
	}
	public void setArxiuImatgePrincipal(Arxiu arxiuImatgePrincipal) {
		this.arxiuImatgePrincipal = arxiuImatgePrincipal;
	}
	public byte[] getImatgeImatgePrincipal() {
		return imatgeImatgePrincipal;
	}
	public void setImatgeImatgePrincipal(byte[] imatgeImatgePrincipal) {
		this.imatgeImatgePrincipal = imatgeImatgePrincipal;
	}
	public Long getImatgePrincipal() {
		return imatgePrincipal;
	}
	public void setImatgePrincipal(Long imatgePrincipal) {
		this.imatgePrincipal = imatgePrincipal;
	}
	public Arxiu getArxiuImatgeSecundaria() {
		return arxiuImatgeSecundaria;
	}
	public void setArxiuImatgeSecundaria(Arxiu arxiuImatgeSecundaria) {
		this.arxiuImatgeSecundaria = arxiuImatgeSecundaria;
	}
	public byte[] getImatgeImatgeSecundaria() {
		return imatgeImatgeSecundaria;
	}
	public void setImatgeImatgeSecundaria(byte[] imatgeImatgeSecundaria) {
		this.imatgeImatgeSecundaria = imatgeImatgeSecundaria;
	}
	public Long getImatgeSecundaria() {
		return imatgeSecundaria;
	}
	public void setImatgeSecundaria(Long imatgeSecundaria) {
		this.imatgeSecundaria = imatgeSecundaria;
	}	
	

	public String getErrorImagenes() {
		return errorImagenes;
	}
	public void setErrorImagenes(String errorImagenes) {
		this.errorImagenes = errorImagenes;
	}

	
}
