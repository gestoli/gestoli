package es.caib.gestoli.logic.model;

public class QualitatAPPCCEtapaPerill {

	private static final long serialVersionUID = 1L;
	
	private Long id;
	private String tipus;
	private String detall;
	private String causa;
	private String prevencio;
	private Long probabilitat;
	private Long gravetat;
	
	private QualitatAPPCCEtapa etapa;
	
	public QualitatAPPCCEtapaPerill(){
		super();
	}
	
	public QualitatAPPCCEtapaPerill(Long id, String tipus, String detall,
			String causa, String prevencio, Long probabilitat, Long gravetat,
			QualitatAPPCCEtapa etapa){
		this.id = id;
		this.tipus = tipus;
		this.detall = detall;
		this.causa = causa;
		this.prevencio = prevencio;
		this.probabilitat = probabilitat;
		this.gravetat = gravetat;
		this.etapa = etapa;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getTipus() {
		return tipus;
	}

	public void setTipus(String tipus) {
		this.tipus = tipus;
	}

	public String getDetall() {
		return detall;
	}

	public void setDetall(String detall) {
		this.detall = detall;
	}

	public String getCausa() {
		return causa;
	}

	public void setCausa(String causa) {
		this.causa = causa;
	}

	public String getPrevencio() {
		return prevencio;
	}

	public void setPrevencio(String prevencio) {
		this.prevencio = prevencio;
	}

	public Long getProbabilitat() {
		return probabilitat;
	}

	public void setProbabilitat(Long probabilitat) {
		this.probabilitat = probabilitat;
	}

	public Long getGravetat() {
		return gravetat;
	}

	public void setGravetat(Long gravetat) {
		this.gravetat = gravetat;
	}

	public QualitatAPPCCEtapa getEtapa() {
		return etapa;
	}

	public void setEtapa(QualitatAPPCCEtapa etapa) {
		this.etapa = etapa;
	}
	
}
