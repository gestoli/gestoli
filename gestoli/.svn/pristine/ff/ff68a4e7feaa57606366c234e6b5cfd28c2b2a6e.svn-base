package es.caib.gestoli.logic.model;

import java.io.Serializable;


public class QualitatDescripcioInstalacio implements Serializable {

	private static final long serialVersionUID = 1L;

	private Long id;
	private Integer sales;
	private Integer plantes;
	private Integer ample;
	private Integer llarg;
	private Integer vestuaris;
	private Integer banys;
	private Integer oficines;
	
	private Establiment establiment;
	
	
	
	public QualitatDescripcioInstalacio() {
		super();
	}
	
	
	
	public QualitatDescripcioInstalacio(Establiment establiment, Integer sales, Integer plantes,
			Integer ample, Integer llarg, Integer vestuaris, Integer banys,
			Integer oficines) {
		super();
		this.establiment = establiment;
		this.sales = sales;
		this.plantes = plantes;
		this.ample = ample;
		this.llarg = llarg;
		this.vestuaris = vestuaris;
		this.banys = banys;
		this.oficines = oficines;
	}



	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public Integer getSales() {
		return sales;
	}
	public void setSales(Integer sales) {
		this.sales = sales;
	}
	public Integer getPlantes() {
		return plantes;
	}
	public void setPlantes(Integer plantes) {
		this.plantes = plantes;
	}
	public Integer getAmple() {
		return ample;
	}
	public void setAmple(Integer ample) {
		this.ample = ample;
	}
	public Integer getLlarg() {
		return llarg;
	}
	public void setLlarg(Integer llarg) {
		this.llarg = llarg;
	}
	public Integer getVestuaris() {
		return vestuaris;
	}
	public void setVestuaris(Integer vestuaris) {
		this.vestuaris = vestuaris;
	}
	public Integer getBanys() {
		return banys;
	}
	public void setBanys(Integer banys) {
		this.banys = banys;
	}
	public Integer getOficines() {
		return oficines;
	}
	public void setOficines(Integer oficines) {
		this.oficines = oficines;
	}
	public Establiment getEstabliment() {
		return establiment;
	}
	public void setEstabliment(Establiment establiment) {
		this.establiment = establiment;
	}
	
}
