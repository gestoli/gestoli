package es.caib.gestoli.logic.model;

import java.util.Date;

public class QualitatProveidorAvaluacio extends QualitatControl {

	private static final long serialVersionUID = 1L;
	private QualitatProveidor proveidor;
	private Integer valoracio;
	private Date dataRehomologacio;
	private Date dataDeshomologacio;
	private Date proximaAvaluacio;
	private String observacions;
	
	public QualitatProveidorAvaluacio(){
		super();
	}
	
	
	public QualitatProveidorAvaluacio(Long id, Integer valoracio, Date dataVerificacio,
			Date dataRehomologacio, Date dataDeshomologacio, Date proximaAvaluacio,
			QualitatDescripcioPersonal responsableVerificacio,
			Boolean satisfactori, QualitatProveidor proveidor, String observacions){
		this.id = id;
		this.valoracio = valoracio;
		this.dataVerificacio = dataVerificacio;
		this.proximaAvaluacio = proximaAvaluacio;
		this.dataRehomologacio = dataRehomologacio;
		this.dataDeshomologacio = dataDeshomologacio;
		this.responsableVerificacio = responsableVerificacio;
		this.satisfactori = satisfactori;
		this.proveidor = proveidor;
		this.observacions = observacions;
	}

	public QualitatProveidor getControlPlaga() {
		return proveidor;
	}
	public void setControlPlaga(QualitatProveidor proveidor) {
		this.proveidor = proveidor;
	}


	public QualitatProveidor getProveidor() {
		return proveidor;
	}


	public void setProveidor(QualitatProveidor proveidor) {
		this.proveidor = proveidor;
	}


	public Integer getValoracio() {
		return valoracio;
	}


	public void setValoracio(Integer valoracio) {
		this.valoracio = valoracio;
	}


	public Date getDataRehomologacio() {
		return dataRehomologacio;
	}


	public void setDataRehomologacio(Date dataRehomologacio) {
		this.dataRehomologacio = dataRehomologacio;
	}


	public Date getDataDeshomologacio() {
		return dataDeshomologacio;
	}


	public void setDataDeshomologacio(Date dataDeshomologacio) {
		this.dataDeshomologacio = dataDeshomologacio;
	}


	public Date getProximaAvaluacio() {
		return proximaAvaluacio;
	}


	public void setProximaAvaluacio(Date proximaAvaluacio) {
		this.proximaAvaluacio = proximaAvaluacio;
	}


	public String getObservacions() {
		return observacions;
	}


	public void setObservacions(String observacions) {
		this.observacions = observacions;
	}
	
	
	
}
