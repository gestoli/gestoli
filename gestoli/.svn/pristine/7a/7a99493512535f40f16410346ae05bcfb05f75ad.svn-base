package es.caib.gestoli.logic.model;

import java.util.Date;

public class QualitatAPPCCFitxaControlHistoric {

	private static final long serialVersionUID = 1L;
	
	private Long id;
	
	// Prevencio
	private String mesuresPrevencio;
	private QualitatDescripcioPersonal responsablePrevencio;
	
	// Vigilancia
	private String vigilancia;
	private QualitatDescripcioPersonal responsableVigilancia;
	private String registre;
	private String limits;
	private String freqVigilancia;
	
	// Correccio
	private String mesuresCorreccio;
	private QualitatDescripcioPersonal responsableCorreccio;	
	
	private QualitatAPPCCFitxaControl fitxa;
	private Date dataModificacio;
	
	public QualitatAPPCCFitxaControlHistoric(){
		super();
	}
	
	public QualitatAPPCCFitxaControlHistoric(
			Long id,
			String mesuresPrevencio,
			QualitatDescripcioPersonal responsablePrevencio,
			String vigilancia,
			QualitatDescripcioPersonal responsableVigilancia,
			String registre,
			String limits,
			String freqVigilancia,
			String mesuresCorreccio,
			QualitatDescripcioPersonal responsableCorreccio,	
			QualitatAPPCCFitxaControl fitxa,
			Date dataModificacio){
		this.id = id;
		this.mesuresPrevencio = mesuresPrevencio;
		this.responsablePrevencio = responsablePrevencio;
		this.vigilancia = vigilancia;
		this.responsableVigilancia = responsableVigilancia;
		this.registre = registre;
		this.limits = limits;
		this.freqVigilancia = freqVigilancia;
		this.mesuresCorreccio = mesuresCorreccio;
		this.responsableCorreccio = responsableCorreccio;
		this.fitxa = fitxa;
		this.dataModificacio = dataModificacio;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getMesuresPrevencio() {
		return mesuresPrevencio;
	}

	public void setMesuresPrevencio(String mesuresPrevencio) {
		this.mesuresPrevencio = mesuresPrevencio;
	}

	public QualitatDescripcioPersonal getResponsablePrevencio() {
		return responsablePrevencio;
	}

	public void setResponsablePrevencio(
			QualitatDescripcioPersonal responsablePrevencio) {
		this.responsablePrevencio = responsablePrevencio;
	}

	public String getVigilancia() {
		return vigilancia;
	}

	public void setVigilancia(String vigilancia) {
		this.vigilancia = vigilancia;
	}

	public QualitatDescripcioPersonal getResponsableVigilancia() {
		return responsableVigilancia;
	}

	public void setResponsableVigilancia(
			QualitatDescripcioPersonal responsableVigilancia) {
		this.responsableVigilancia = responsableVigilancia;
	}

	public String getRegistre() {
		return registre;
	}

	public void setRegistre(String registre) {
		this.registre = registre;
	}

	public String getLimits() {
		return limits;
	}

	public void setLimits(String limits) {
		this.limits = limits;
	}

	public String getFreqVigilancia() {
		return freqVigilancia;
	}

	public void setFreqVigilancia(String freqVigilancia) {
		this.freqVigilancia = freqVigilancia;
	}

	public String getMesuresCorreccio() {
		return mesuresCorreccio;
	}

	public void setMesuresCorreccio(String mesuresCorreccio) {
		this.mesuresCorreccio = mesuresCorreccio;
	}

	public QualitatDescripcioPersonal getResponsableCorreccio() {
		return responsableCorreccio;
	}

	public void setResponsableCorreccio(
			QualitatDescripcioPersonal responsableCorreccio) {
		this.responsableCorreccio = responsableCorreccio;
	}

	public QualitatAPPCCFitxaControl getFitxa() {
		return fitxa;
	}

	public void setFitxa(QualitatAPPCCFitxaControl fitxa) {
		this.fitxa = fitxa;
	}

	public Date getDataModificacio() {
		return dataModificacio;
	}

	public void setDataModificacio(Date dataModificacio) {
		this.dataModificacio = dataModificacio;
	}

	
	
}
