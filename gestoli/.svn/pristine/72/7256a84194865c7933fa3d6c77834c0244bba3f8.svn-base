package es.caib.gestoli.logic.model;

import java.io.Serializable;
import java.util.Date;
import java.util.Set;


public class QualitatPlaNeteja implements Serializable {


	private static final long serialVersionUID = 1L;

	private Long codi;
	private QualitatDescripcioEquip equip;
	private String elementPlanta;
	private String accio;
	private Boolean esGeneral;  // false -> neteja d'equips       true -> neteja general de planta
	private QualitatSubministre producte;
	private String dosis;
	private String frequencia;
	private QualitatDescripcioPersonal responsable;
	private QualitatDescripcioPersonal respVerificacio;
	private String periodicitatVerificacio;
	private Establiment establiment;
	
	
	public QualitatPlaNeteja() {
		super();
	}


	public QualitatPlaNeteja(Long codi, QualitatDescripcioEquip equip,
			String planta, String accio, Boolean tipusNetetja, Long numRGSA,
			QualitatSubministre producte, String dosis, String frequencia,
			QualitatDescripcioPersonal responsable,
			QualitatDescripcioPersonal respVerificacio,
			String periodicitatVerificacio, Establiment establiment) {
		super();
		this.codi = codi;
		this.equip = equip;
		this.elementPlanta = planta;
		this.accio = accio;
		this.esGeneral = tipusNetetja;
		this.producte = producte;
		this.dosis = dosis;
		this.frequencia = frequencia;
		this.responsable = responsable;
		this.respVerificacio = respVerificacio;
		this.periodicitatVerificacio = periodicitatVerificacio;
		this.establiment = establiment;
	}





	public QualitatDescripcioEquip getEquip() {
		return equip;
	}


	public void setEquip(QualitatDescripcioEquip equip) {
		this.equip = equip;
	}


	public String getPlanta() {
		return elementPlanta;
	}


	public void setPlanta(String planta) {
		this.elementPlanta = planta;
	}


	public Long getCodi() {
		return codi;
	}


	public void setCodi(Long codi) {
		this.codi = codi;
	}


	public String getAccio() {
		return accio;
	}


	public void setAccio(String accio) {
		this.accio = accio;
	}


	public Boolean getEsGeneral() {
		return esGeneral;
	}


	public void setEsGeneral(Boolean tipusNeteja) {
		this.esGeneral = tipusNeteja;
	}



	public QualitatSubministre getProducte() {
		return producte;
	}


	public void setProducte(QualitatSubministre producte) {
		this.producte = producte;
	}


	public String getDosis() {
		return dosis;
	}


	public void setDosis(String dosis) {
		this.dosis = dosis;
	}


	public String getFrequencia() {
		return frequencia;
	}


	public void setFrequencia(String frequencia) {
		this.frequencia = frequencia;
	}


	public QualitatDescripcioPersonal getResponsable() {
		return responsable;
	}


	public void setResponsable(QualitatDescripcioPersonal responsable) {
		this.responsable = responsable;
	}


	public QualitatDescripcioPersonal getRespVerificacio() {
		return respVerificacio;
	}


	public void setRespVerificacio(QualitatDescripcioPersonal respVerificacio) {
		this.respVerificacio = respVerificacio;
	}


	public String getPeriodicitatVerificacio() {
		return periodicitatVerificacio;
	}


	public void setPeriodicitatVerificacio(String periodicitatVerificacio) {
		this.periodicitatVerificacio = periodicitatVerificacio;
	}


	public Establiment getEstabliment() {
		return establiment;
	}


	public void setEstabliment(Establiment establiment) {
		this.establiment = establiment;
	}





	public String getElementPlanta() {
		return elementPlanta;
	}





	public void setElementPlanta(String elementPlanta) {
		this.elementPlanta = elementPlanta;
	}
	
	
	
	
}
	
	
