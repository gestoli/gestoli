package es.caib.gestoli.logic.model;

public class QualitatAPPCCControl extends QualitatControl {

	private static final long serialVersionUID = 1L;
	
	private Boolean p1;
	private Boolean p2;
	private Boolean p3;
	private Boolean p4;
	private Boolean p5;
	private String puntControl;
	private String perillControlat;
	
	private QualitatAPPCCEtapaPerill perill;
	private QualitatAPPCCEtapa etapa;

	public QualitatAPPCCControl(){
		super();
	}
	
	public QualitatAPPCCControl(Long id, Boolean p1, Boolean p2,
			Boolean p3, Boolean p4, Boolean p5, String puntControl,
			String perillControlat){
		this.id = id;
		this.p1 = p1;
		this.p2 = p2;
		this.p3 = p3;
		this.p4 = p4;
		this.p5 = p5;
		this.puntControl = puntControl;
		this.perillControlat = perillControlat;
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Boolean getP1() {
		return p1;
	}

	public void setP1(Boolean p1) {
		this.p1 = p1;
	}

	public Boolean getP2() {
		return p2;
	}

	public void setP2(Boolean p2) {
		this.p2 = p2;
	}

	public Boolean getP3() {
		return p3;
	}

	public void setP3(Boolean p3) {
		this.p3 = p3;
	}

	public Boolean getP4() {
		return p4;
	}

	public void setP4(Boolean p4) {
		this.p4 = p4;
	}

	public Boolean getP5() {
		return p5;
	}

	public void setP5(Boolean p5) {
		this.p5 = p5;
	}

	public String getPuntControl() {
		return puntControl;
	}

	public void setPuntControl(String puntControl) {
		this.puntControl = puntControl;
	}

	public String getPerillControlat() {
		return perillControlat;
	}

	public void setPerillControlat(String perillControlat) {
		this.perillControlat = perillControlat;
	}

	public QualitatAPPCCEtapaPerill getPerill() {
		return perill;
	}

	public void setPerill(QualitatAPPCCEtapaPerill perill) {
		this.perill = perill;
	}

	public QualitatAPPCCEtapa getEtapa() {
		return etapa;
	}

	public void setEtapa(QualitatAPPCCEtapa etapa) {
		this.etapa = etapa;
	}
	
	
}
