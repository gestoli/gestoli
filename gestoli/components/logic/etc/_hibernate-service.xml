<?xml version="1.0" encoding="UTF-8"?>

<server>
	<mbean code="org.jboss.hibernate.jmx.Hibernate" name="jboss.har:service=HibernateGestOli">
		<attribute name="DatasourceName">java:/GestOliDS</attribute>
		<attribute name="SessionFactoryName">java:/hibernate/GestOliSessionFactory</attribute>
		<attribute name="Dialect">net.sf.hibernate.dialect.PostgreSQLDialect</attribute>
		<attribute name="CacheProviderClass">net.sf.hibernate.cache.HashtableCacheProvider</attribute>
		<!--attribute name="Hbm2ddlAuto">update</attribute-->
		<attribute name="ShowSqlEnabled">true</attribute>
	</mbean>
</server>